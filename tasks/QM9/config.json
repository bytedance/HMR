{   
    "run_name": "runX",
    "out_dir": "output",
    "test_freq": 10,
    "seed": 2023,
    "auto_resume": true,
    "mute_tqdm": false,
    
    "data_dir": "",
    "batch_size": 32,
    "num_data_workers": 8,
    "target": "homo",
    "num_gdf": 8,
    "cutoff_radius": 6.0,

    "optimizer": "AdamW",
    "epochs": 300,
    "warmup_epochs": 10,
    "lr": 5E-4,
    "lr_scheduler": "PolynomialLRWithWarmup",
    "weight_decay": 0.0,
    "clip_grad_norm": 1.0,
    "fp16": false,

    "model": "HMR",
    "HMR": {
        "h_dim": 128,
        "num_prop_layers": 6,
        "init_prop_time": 2.0,
        "propagation_time_scale": 10.0
    }
}
