{   
    "run_name": "runX",
    "out_dir": "output",
    "upload_freq": 10,
    "test_freq": 1,
    "seed": 2023,
    "auto_resume": true,
    "mute_tqdm": true,
    
    "data_dir": "",
    "batch_size": 2,
    "num_data_workers": 2,
    "cv": 0,

    "optimizer": "AdamW",
    "epochs": 100,
    "warmup_epochs": 10,
    "lr": 5E-4,
    "lr_scheduler": "PolynomialLRWithWarmup",
    "weight_decay": 0.0,
    "clip_grad_norm": 1.0,
    "fp16": false,
    "restore": "",

    "model": "HMR",
    "HMR": {
        "fp16": true,
        "iface_cutoff": 3.0,
        "lb_basis_cutoff": 0.06,
        "vert_nbr_atoms": 16,
        "num_gdf": 16,
        "num_signatures": 16,
        "h_dim": 128,
        "chem_embed_dim": 32,
        "geom_feat_dim": 32,
        "chem_aggr": "mean",
        "num_message_passing_blocks": 2,
        "num_propagation_layers": 3,
        "propagation_time_scale": 10.0,
        "apply_band_filter": true,
        "band_e_mean": 0.1,
        "band_e_std": 0.1,
        "h_dim_div": 1,
        "num_cross_attn_layers": 1,
        "num_attn_heads": 4,
        "num_smoothing_layers": 1,
        "bsp_loss": "focal",
        "focal_gamma": 2,
        "focal_alpha": 0.25,
        "nce_loss": true,
        "nce_loss_weight": 0.1,
        "nce_num_samples": 50,
        "nce_T": 10.0,
        "dropout": 0.1,
        "attn_dropout": 0.0
    }
}
